<h1> All posts </h1>

<ul>
  <% @posts.each do | post| %>
    <li>
      ID = <%= post.id%> <a href="/delete/<%= post.id%>"> delete post </a>
      <p></p>
      Title: <%= post.title%>
      <p></p>
      URL: <%= post.url%>
      <p></p>
      Votes: <%= post.votes%> <a href="/upvote/<%= post.id%>"> upvote </a>
    </li>
    <p></p>
    <p></p>
  <% end %>
</ul>
<a href="/create_post"> Create a new post </a>
